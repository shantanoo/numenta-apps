{
  "name": "unicorn",
  "version": "0.0.1",
  "description": "Numenta Unicorn Cross-platform HTM Example Desktop Application",
  "main": "frontend/loader.js",
  "engines": {
    "node": ">=0.12.7",
    "npm": ">=2.11.3"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64"
  ],
  "scripts": {
    "check": "npm install && npm prune && npm outdated -depth 0",
    "clean": "rm -Rf node_modules/",
    "desktop": "npm install && npm run pack-electron && npm run electron",
    "dev": "npm run web",
    "electron": "electron .",
    "pack-electron": "gulp webpack --target atom",
    "pack-web": "gulp webpack",
    "serve": "http-server -p 9999 ./frontend/browser/",
    "start": "npm run desktop",
    "test": "echo \"Error: no test specified\" && exit 1",
    "web": "npm install && npm run pack-web && npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/numenta/numenta-apps/tree/master/unicorn"
  },
  "keywords": [
    "Unicorn",
    "Numenta",
    "Hierarchical",
    "Temporal",
    "Memory",
    "HTM",
    "Machine",
    "Intelligence",
    "Desktop",
    "Application"
  ],
  "homepage": "http://numenta.com",
  "bugs": "https://github.com/numenta/numenta-apps/issues",
  "author": {
    "name": "Numenta",
    "email": "info@numenta.com",
    "url": "http://numenta.com"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "dependencies": {
    "electron": "0.4.1",
    "fluxible": "0.5.2",
    "fluxible-addons-react": "0.1.6",
    "is-electron-renderer": "2.0.0",
    "jsondown": "^0.1.1",
    "levelup": "^1.2.1",
    "material-ui": "0.11.0",
    "node-sass": "3.2.0",
    "react": "0.13.3",
    "react-tap-event-plugin": "0.1.7"
  },
  "devDependencies": {
    "babel": "5.8.23",
    "babel-core": "5.8.23",
    "babel-eslint": "4.1.0",
    "babel-loader": "5.3.2",
    "babel-plugin-typecheck": "1.2.0",
    "casperjs": "1.1.0-beta3",
    "electron-debug": "0.2.0",
    "electron-prebuilt": "0.31.1",
    "eslint": "^1.3.0",
    "eslint-plugin-react": "3.3.0",
    "gulp": "3.9.0",
    "gulp-util": "3.0.6",
    "http-server": "0.8.0",
    "json-loader": "0.5.2",
    "mocha": "2.2.5",
    "mocha-casperjs": "0.5.4",
    "node-inspector": "0.12.2",
    "webpack": "1.12.0",
    "webpack-stream": "2.1.0"
  }
}
